{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "txt_light": "#93a1a1",
    "bg_default":"#586e75",
    "bg_bright": "#839496",
    "black": "#002b36",
    "white": "#E0DEF4",
    "yellow": "#b58900",
    "orange": "#cb4b16",
    "red": "#dc322f",
    "magenta": "#d33682",
    "violet": "#6c71c4",
    "blue": "#268bd2",
    "cyan": "#2aa198",
    "green": "#859900"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "foreground": "transparent",
          "background": "p:bg_default",
          "template": "{{ .UserName }}"
        },
        {
          "type": "text",
          "style": "diamond",
          "foreground": "transparent",
          "background": "p:bg_default",
          "template": " <i>@</i> "
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "transparent",
          "background": "p:bg_default",
          "template": "{{ .HostName }}"
        },
        {
          "type": "command",
          "style": "diamond",
          "foreground": "p:txt_light",
          "background": "p:bg_default",
          "template": ":<b>{{ .Output }}</b> ",
          "properties": {
              "command": "ps -otty= $$"
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "background": "p:bg_bright",
          "foreground": "transparent",
          "template": " <b>{{ .Path }}</b>",
          "properties": {
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .UpstreamIcon }}<b>{{ .HEAD }}</b>{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<p:black> \uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<> \uf046 {{ .Staging.String }}</>{{ end }}",
          "foreground": "p:white",
          "background": "p:green",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:red{{ end }}",
            "{{ if or (gt .Ahead 0) (gt .Behind 0) }}p:blue{{ end }}"
          ],
          "properties": {
            "branch_max_length": 30,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "background": "p:red",
          "foreground": "p:white",
          "template": "{{if .Env.BG_JOBS}}\ufe60{{.Env.BG_JOBS}} {{end}}"
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "transparent",
          "background": "p:bg_default",
          "template": "{{ .CurrentDate | date .Format }}",
          "properties": {
            "time_format": "2006-01-02 15:04:05"
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:white",
          "background": "p:bg_bright",
          "template": " {{if eq \"Discharging\" .State.String}}{{ if not .Error }}{{ .Icon }}{{ .Percentage }}%{{ end }}{{end}}",
          "background_templates": [
            "{{if eq \"Discharging\" .State.String}}{{if le .Percentage 10}}p:red{{else}}{{if le .Percentage 20}}p:yellow{{end}}{{end}}{{end}}"
          ],
          "properties": {
            "discharging_icon": " ",
            "charging_icon": " ",
            "charged_icon": " "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "foreground": "p:bg_default",
          "template": "\u2570\u2500"
        },
        {
          "type": "shell",
          "style": "diamond",
          "foreground": "p:txt_light",
          "template": " {{ .Name }}"
        },
        {
          "type": "python",
          "style": "diamond",
          "foreground": "p:yellow",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}"
        },
        {
          "type": "text",
          "style": "diamond",
          "foreground": "p:txt_light",
          "template": " \u276f"
        }
      ]
    }
  ],
  "final_space": true,
  "version": 3
}
